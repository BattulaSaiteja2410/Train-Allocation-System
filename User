package PaidIntership;

public class User {
	private String id;
    private String name;
    private int age;
    private int selectedTrain;
    private int selectedBoogie;
    private int selectedSeat;

    public User(String id, String name, int age, int selectedTrain, int selectedBoogie, int selectedSeat) {
        this.id = id;
        this.name = name;
        this.age = age;
        this.selectedTrain = selectedTrain;
        this.selectedBoogie = selectedBoogie;
        this.selectedSeat = selectedSeat;
    }

    public String getId() {
        return id;
    }

    public String getName() {
        return name;
    }

    public int getAge() {
        return age;
    }

    public int getSelectedTrain() {
        return selectedTrain;
    }

	public int getSelectedBoogie() {
		
		return selectedBoogie;
	}
public int getSelectedSeat() {
		
		return selectedSeat;
	}
}
